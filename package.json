  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env BASE_URL=http://localhost:3000 react-scripts test",
    "test:e2e": "cross-env BASE_URL=http://localhost:3000 jest --outputFile=jest-results.json --json --config=jest-e2e.config.json",
    "test:e2e:browserstack": "cross-env BASE_URL=http://localhost:8099  BROWSERSTACK=true browserstack-node-sdk jest --outputFile=jest-results.json --json --config=jest-e2e.config.json  ",
    "test:firestore:rules": "firebase emulators:exec --only firestore \"npm run test:firestore:rules:run \"",
    "test:firestore:rules:run": "jest --detectOpenHandles --forceExit --config=firebase.jest.config.json",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.ts{,x}",
    "lint:fix": "eslint src/**/*.ts{,x} --fix",
    "prettier:fix": "npx prettier --write .",
    "prettier": "npx prettier --check ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
